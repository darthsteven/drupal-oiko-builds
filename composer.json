{
    "name": "computerminds/oiko",
    "description": "Oiko structure data platform",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "ComputerMinds",
            "email": "info@computerminds.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/eva",
                "version": "dev-8.x-1.x",
                "type": "drupal-module",
                "require": {
                    "composer/installers": "^1.0.21"
                },
                "source": {
                    "type": "git",
                    "url": "https://git.drupal.org/project/eva.git",
                    "reference": "origin/8.x-1.x"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/pathauto",
                "version": "dev-8.x-1.x",
                "type": "drupal-module",
                "require": {
                    "composer/installers": "^1.0.21"
                },
                "source": {
                    "type": "git",
                    "url": "https://git.drupal.org/project/pathauto.git",
                    "reference": "05a0bc51694b70efb697b3c8b175f2884bff7b34"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/leaflet_widget",
                "version": "dev-8.x-2.x",
                "type": "drupal-module",
                "require": {
                    "composer/installers": "^1.0.21"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/computerminds/leaflet_widget.git",
                    "reference": "origin/8.x-1.x"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "arthur-e/Wicket",
                "version": "1.3.2",
                "type": "drupal-library",
                "require": {
                    "composer/installers": "^1.0.21"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/arthur-e/Wicket.git",
                    "reference": "1.3.2"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "Leaflet/Leaflet.draw",
                "version": "0.3.0",
                "type": "drupal-library",
                "require": {
                    "composer/installers": "^1.0.21"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/Leaflet/Leaflet.draw",
                    "reference": "v0.3.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "Leaflet/Leaflet",
                "version": "0.7.7",
                "type": "drupal-library",
                "require": {
                    "composer/installers": "^1.0.21"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/Leaflet/Leaflet.git",
                    "reference": "v0.7.7"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "require": {
        "arthur-e/Wicket": "^1.3.2",
        "Leaflet/Leaflet": "^0.7.7",
        "Leaflet/Leaflet.draw": "^0.3.0",
        "composer/installers": "^1.0.21",
        "oomphinc/composer-installers-extender": "dev-master",
        "drupal-composer/drupal-scaffold": "^1.3.1",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "~8.1",
        "drush/drush": "~8.0",
        "drupal/add_to_head": "dev-8.x-1.x",
        "drupal/admin_toolbar": "^8.1.14",
        "drupal/config_readonly": "8.1.0-beta2",
        "drupal/console": "~0.10",
        "drupal/config_devel": "8.1.0-rc1",
        "drupal/csv_serialization": "8.1.0-alpha1",
        "drupal/devel": "8.1.x-dev",
        "drupal/eva": "dev-8.x-1.x",
        "drupal/features": "8.3.x-dev",
        "drupal/field_group": "8.1.x-dev",
        "drupal/geofield": "8.1.0-alpha2",
        "drupal/leaflet": "8.1.x-dev",
        "drupal/leaflet_widget": "dev-8.x-2.x",
        "drupal/metatag": "8.1.0-beta7",
        "drupal/migrate_plus": "8.2.0-beta1",
        "drupal/migrate_tools": "8.2.x-dev",
        "drupal/migrate_upgrade": "8.2.0-beta1",
        "drupal/squid_autocomplete": "8.1.0-alpha5",
        "drupal/paragraphs": "^8.1@RC",
        "drupal/pathauto": "dev-8.x-1.x",
        "wikimedia/composer-merge-plugin": "^1.3",
        "drupal/geocoder": "8.2.0-alpha4",
        "drupal/geocoder_autocomplete": "8.1.0-alpha1",
        "drupal/ctools": "8.3.0-alpha26",
        "drupal/browsersync": "8.1.0-beta1",
        "drupal/color_field": "8.2.0-rc1"
    },
    "require-dev": {
        "behat/mink": "~1.6",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "^1.1@dev",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "2.7.*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::buildScaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::buildScaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "merge-plugin": {
            "include": [
                 "webroot/modules/custom/cidoc_spec/composer.json"
                ,"webroot/modules/custom/cidoc_sync/composer.json"
                ,"webroot/modules/custom/edtf/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-extra": false
        },
        "installer-types": ["drupal-library"],
        "installer-paths": {
            "webroot/core": [
                "type:drupal-core"
            ],
            "webroot/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "webroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "webroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "webroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/core": {
                "Prevent a notice from views_query_views_alter": "https://www.drupal.org/files/issues/2744069-5-views-in-queries.patch"
            },
            "drupal/add_to_head": {
                "Add applies method patch": "https://www.drupal.org/files/issues/checkfordefinitiontype-2709377.patch"
            },
            "drupal/paragraphs": {
                "Allow using paragraphs on non-translatable entities": "https://www.drupal.org/files/issues/paragraphs-2755897-cannot-use-paragraphs-on-non-translatable-entity-rc5_0.patch"
            },
            "drupal/leaflet": {
                "New version of geofield uses composer for geophp dependency, geophp_load not needed" : "https://www.drupal.org/files/issues/leaflet-remove_geophp_load_call-2731917-2.patch",
                "Fixes Incorrect parameter icon_url in Drupal.Leaflet.prototype.create_icon" : "https://www.drupal.org/files/issues/leaflet-fix_incorrect_parameter_name_in_create_icon-2732267-2.patch",
                "Fixes maps not working on HTTPS": "https://www.drupal.org/files/issues/leaflet-ssl_support-2753703-2_0.patch",
                "Fixes the API docs to include a missing hook": "https://www.drupal.org/files/issues/leaflet-add-hook_leaflet_map_info_alter-2746721-2.patch",
                "Allows use of geometrycollection": "https://www.drupal.org/files/issues/leaflet-2799311-geometry-collection.patch"
            },
            "drupal/eva": {
                "EVA Patch for D8" : "https://www.drupal.org/files/issues/eva-working-d8-1937618-21.patch"
            }
        }
    }
}
