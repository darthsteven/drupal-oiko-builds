{
    "name": "computerminds/oiko",
    "description": "Oiko structure data platform",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [{
        "name": "ComputerMinds",
        "email": "info@computerminds.co.uk"
    }],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/leaflet_widget",
                "version": "dev-8.x-2.x",
                "type": "drupal-module",
                "require": {
                    "composer/installers": "^2.1"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/computerminds/leaflet_widget.git",
                    "reference": "origin/8.x-1.x"
                }
            }
        },
        {
            "type": "git",
            "url": "https://github.com/digihum/cidoc-crm.git"
        },
        {
            "type": "package",
            "package": {
                "name": "arthur-e/wicket",
                "version": "1.3.8",
                "type": "drupal-library",
                "require": {
                    "composer/installers": "^2.1"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/arthur-e/Wicket.git",
                    "reference": "1.3.8"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "leaflet/leaflet.draw",
                "version": "0.3.0",
                "type": "drupal-library",
                "require": {
                    "composer/installers": "^2.1"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/Leaflet/Leaflet.draw",
                    "reference": "v0.3.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "leaflet/leaflet",
                "version": "1.0.3",
                "type": "drupal-library",
                "require": {
                    "composer/installers": "^2.1"
                },
                "dist": {
                    "url": "https://github.com/Leaflet/Leaflet/archive/v1.0.3.zip",
                    "type": "zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/Leaflet/Leaflet.git",
                    "reference": "v1.0.3"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "ext-json": "*",
        "arthur-e/wicket": "^1.3.2",
        "composer/installers": "^2.1",
        "cweagans/composer-patches": "^1.6",
        "digihum/cidoc-crm": "^1.1",
        "drupal/add_to_head": "^1.0@beta",
        "drupal/admin_toolbar": "^3",
        "drupal/avatars": "^1.0@beta",
        "drupal/basicshib": "^2.0",
        "drupal/browsersync": "^3.0@beta",
        "drupal/color_field": "^3",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/config_split": "^2.0@RC",
        "drupal/config_update": "^1.7",
        "drupal/core-composer-scaffold": "^9.5",
        "drupal/core-project-message": "^9.5",
        "drupal/core-recommended": "^9.5",
        "drupal/css_editor": "^2.0",
        "drupal/csv_serialization": "^2.1",
        "drupal/ctools": "^3.0",
        "drupal/editor_file": "^1.5",
        "drupal/entity_reference_revisions": "^1.10",
        "drupal/entity_share": "^3.0",
        "drupal/eu_cookie_compliance": "^1.0",
        "drupal/eva": "^2.0",
        "drupal/field_group": "^3.0.0",
        "drupal/geocoder": "^3.34||^4.9",
        "drupal/geocoder_autocomplete": "^2.0",
        "drupal/geofield": "^1.6",
        "drupal/geophp": "^1.1",
        "drupal/google_analytics": "^2.1",
        "drupal/honeypot": "^2.1",
        "drupal/http_auth": "^2.0",
        "drupal/key_auth": "^1.1",
        "drupal/leaflet": "1.x-dev#a3274e4",
        "drupal/leaflet_widget": "dev-8.x-2.x",
        "drupal/metatag": "^1.26||^2.0",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_upgrade": "^4.0",
        "drupal/paragraphs": "^1",
        "drupal/pathauto": "^1.8",
        "drupal/r4032login": "^2.2",
        "drupal/search_api": "^1.0",
        "drupal/sharemessage": "1.0-beta4",
        "drupal/squid_autocomplete": "1.0-alpha6",
        "drupal/stage_file_proxy": "*",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_delimited_list": "^2.0@alpha",
        "drupal/views_fields_on_off": "^1.0@alpha",
        "drupal/webform": "^6",
        "drush/drush": "^11.0",
        "leaflet/leaflet": "1.0.3",
        "leaflet/leaflet.draw": "^0.3.0",
        "oomphinc/composer-installers-extender": "^2.0.1",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "require-dev": {
        "palantirnet/drupal-rector": "^0.15.1",
        "drupal/upgrade_status": "^3.19",
        "drupal/config_devel": "^1.9",
        "drupal/devel": "^4.1",
        "drupal/devel_entity_updates": "^4.0",
        "mglaman/drupal-check": "^1.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "webroot/"
            }
        },
        "composer-exit-on-patch-failure": true,
        "merge-plugin": {
            "include": [
                "webroot/modules/custom/cidoc_spec/composer.json", "webroot/modules/custom/cidoc_sync/composer.json", "webroot/modules/custom/edtf/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-extra": false
        },
        "installer-types": ["drupal-library"],
        "installer-paths": {
            "webroot/core": [
                "type:drupal-core"
            ],
            "webroot/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "webroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "webroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "webroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/leaflet": {
                "Fixes the API docs to include a missing hook": "https://www.drupal.org/files/issues/leaflet-add-hook_leaflet_map_info_alter-2746721-2.patch"
            },
            "drupal/field_group": {
                "https://git.drupalcode.org/project/field_group/-/merge_requests/34.diff": "https://git.drupalcode.org/project/field_group/-/merge_requests/34.diff"
            },
            "drupal/entity_share": {
                "Allow notification of entity changes": "https://www.drupal.org/files/issues/2020-07-30/entity_share-automatic_synchronization-WIP-3080629-11.patch"
            }
        },
        "composer-version": "^1.8.4"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.29"
        },
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "wikimedia/composer-merge-plugin": true
        }
    }
}
